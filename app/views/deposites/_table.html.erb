<table>
  <col width="100%" />
	<thead>
		<tr>
	    <th><%= Deposite.human_attribute_name('asset') %></th>
	    <th><%= Deposite.human_attribute_name('quantity') %></th>
	  </tr>
	</thead>
	<tbody>
		<% deposites.each do |deposite| %>
