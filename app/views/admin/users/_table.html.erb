<table>
	<thead>
	  <tr>
	    <th><%= User.human_attribute_name('name') %></th>
	    <th><%= User.human_attribute_name('guild') %></th>
			<th><%= User.human_attribute_name('email') %></th>
			<th><%= User.human_attribute_name('pigMoneyBox') %></th>
			<th><%= User.human_attribute_name('gatherer') %></th>
	    <th><%= User.human_attribute_name('admin') %></th>
	  </tr>
	</thead>
	<tbody>
		<% users.each do |user| %>
		  <%= render :partial => 'admin/users/row', :locals => { :user => user } %>
		<% end %>
	</tbody>
</table>
